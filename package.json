{
  "name": "looks-rare-interface",
  "version": "0.0.1",
  "private": false,
  "dependencies": {
    "@ethersproject/abi": "^5.0.12",
    "@ethersproject/experimental": "^5.0.9",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.5.0",
    "@sushiswap/sdk": "^5.0.0-canary.14",
    "@web3-react/abstract-connector": "^6.0.7",
    "@web3-react/core": "^6.1.1",
    "@web3-react/fortmatic-connector": "^6.1.6",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/portis-connector": "^6.1.6",
    "@web3-react/types": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.1.6",
    "@web3-react/walletlink-connector": "^6.1.6",
    "apollo-boost": "^0.4.9",
    "bnc-notify": "^1.5.1",
    "bnc-onboard": "^1.37.0", 
    "classnames": "^2.2.6",
    "eslint-import-resolver-typescript": "^2.3.0",
    "ethers": "^5.3.0",
    "ethers-multicall": "^0.1.6",
    "i18next": "^19.8.9",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.20",
    "notistack": "^1.0.9",
    "react": "^17.0.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.7",
    "react-lottie": "^1.2.3",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "redux-localstorage-simple": "^2.3.1",
    "typescript": "^4.1.5",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts --max_old_space_size=4096 start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "lint": "eslint src",
    "postinstall": "typechain --target ethers-v5 --outDir ./src/contracts \"./src/constants/abi/*.json\""
  },
  "lint-staged": {
    "*.{ts,tsx,json}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@typechain/ethers-v5": "^4.0.0",
    "@types/classnames": "^2.2.11",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.28",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-lottie": "^1.2.5",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/utf8": "^2.1.6",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "typechain": "^4.0.1"
  }
}
